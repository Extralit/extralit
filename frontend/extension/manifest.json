{
  "manifest_version": 3,
  "version": "1.0.0",
  "name": "Extralit",
  "description": "Extralit extension for document data extractions",
  "author": "nhat.c.tran@gmail.com",
  "icons": {
    "128": "icon.png"
  },
  "permissions": ["activeTab", "storage", "scripting"],
  "action": {
    "default_icon": "icon.png",
    "default_title": "Extralit",
    "default_popup": "popup.html"
  }
}
